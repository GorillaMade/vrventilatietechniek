<header class="site-header">
    <div class="site-header__inner wrapper">
        <a class="brand" href="/">
            {{ if site.logo }}
                <img src="{{ site.logo }}" inline>
            {{ /if }}
            <span>
                {{ site.name }}
            </span>
        </a>
        {{ include "partials/shared/nav.vto" }}
    </div>
</header>