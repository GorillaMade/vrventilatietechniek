body {
  background: var(--primary-50);
  color: var(--primary-950);
  font-family: var(--ff-sans);
  font-size: var(--fs-step-0);
  font-weight: var(--fw-regular);
  line-height: 1.45;
}

h1, h2, h3,
h4, h5, h6 {
  letter-spacing: -0.025rem;
  font-family: var(--ff-heading);
  font-weight: var(--fw-bold);
  line-height: 1.2;
  color: inherit;
}
h1:is(:first-of-type), h2:is(:first-of-type), h3:is(:first-of-type),
h4:is(:first-of-type), h5:is(:first-of-type), h6:is(:first-of-type) {
  margin-block-start: 0;
}

h1, .h1 {
  font-size: var(--fs-step-5);
}

h2, .h2 {
  font-size: var(--fs-step-4);
}

h3, .h3 {
  font-size: var(--fs-step-3);
}

h4, .h4 {
  font-size: var(--fs-step-2);
}

h5, .h5 {
  font-size: var(--fs-step-1);
}

h6, .h6 {
  font-size: var(--fs-step-0);
}

a:not([class]) {
  color: var(--accent-500);
}
a:not([class]):is(:hover, :focus-visible) {
  color: var(--accent-800);
}

input:not([type=submit], [type=checkbox], [type=radio]),
textarea {
  min-width: 100%;
  padding: var(--space-xs);
  border: 1px solid var(--primary-200);
  border-radius: var(--radius-s);
}

textarea {
  --textarea-height: 180px;
  min-height: var(--textarea-height);
  height: var(--textarea-height);
  resize: vertical;
}

.cluster {
  display: flex;
  flex-wrap: wrap;
  gap: var(--cluster-gap, var(--space-2xs));
  justify-content: var(--cluster-horizontal-alignment, flex-start);
  align-items: var(--cluster-vertical-alignment, center);
}

.grid {
  display: grid;
  grid-template-columns: repeat(var(--grid-placement, auto-fill), minmax(min(var(--grid-min-item-size, 16rem), 100%), 1fr));
  gap: var(--grid-gap, 1rem);
}

.grid[data-layout=halves] {
  --grid-placement: auto-fit;
  --grid-min-item-size: clamp(16em, 50dvw, 32rem);
}
.grid[data-layout=thirds] {
  --grid-placement: auto-fit;
  --grid-min-item-size: clamp(16em, 33%, 26rem);
}

.site-header__inner {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-between;
  min-height: fit-content;
  padding-block: var(--space-xs);
}

.stack {
  display: grid;
  grid-template-areas: "stack";
  place-content: center;
}
.stack > * {
  grid-area: stack;
}

.switcher {
  display: flex;
  flex-wrap: wrap;
  gap: var(--switcher-gap, var(--space-l-xl));
  align-items: var(--switcher-vertical-alignment, flex-start);
}
.switcher > * {
  flex-grow: 1;
  flex-basis: calc((var(--switcher-target-container-width, 40rem) - 100%) * 999);
}
.switcher > :nth-child(n+3) {
  flex-basis: 100%;
}

.wrapper {
  --_wrapper-max-width: var(--wrapper-max-width, 84rem);
  --_wrapper-padding-inline: var(--wrapper-padding-inline, 5dvw);
  width: min(var(--_wrapper-max-width), 100% - var(--_wrapper-padding-inline) * 2);
  margin-inline: auto;
}

.wrapper--small {
  --wrapper-max-width: 50rem;
}
.wrapper--wide {
  --wrapper-max-width: 62rem;
}

